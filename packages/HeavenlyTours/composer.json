{
    "name": "Heavenly Tours",
    "description": "pulling tours",
    "keywords": [
        "laravel"
    ],
    "homepage": "",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "reza baghiee",
            "email": "baghieereza@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.6"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "HeavenlyTours\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeavenlyTours\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "HeavenlyTours\\ServiceProvider"
            ]
        }
    }
}
